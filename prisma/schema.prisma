generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String  @unique
  money Int
  properties Property[]
}


model Property {
  id          Int    @id @default(autoincrement())
  name String @unique
  user      User?   @relation(fields: [userId], references: [id])
  userId    Int?
}